<div>
  <p>
    You can use this service to:
  </p>

  <ul>
    <li>
      Compare job plans versus actual time spent with patients.
    </li>
    <li>
      Quickly spot service and data collection problems.
    </li>
    <li id='d3-test'>
    </li>
  </ul>

  <% if user_authenticated? %>
    <%= link_to auth_logout_path do %>
      <%= I18n.t('auth.log_out') %> <i class="far fa-arrow-alt-circle-right"></i>
    <% end %>
  <% else %>
    <%= button_to root_url + 'auth/auth0', method: :post, type: 'button', class: 'btn btn-primary' do %>
      <%= I18n.t('auth.log_in') %> <i class="far fa-arrow-alt-circle-right"></i>
    <% end %>
  <% end %>

</div>

<%= javascript_pack_tag 'd3_test', 'data-turbolinks-track': 'reload' %>
