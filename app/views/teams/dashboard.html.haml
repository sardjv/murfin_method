.container.my-4.xs-row-reverse
  = link_to '< Back to Teams', admin_teams_path if @presenter.context == :admin_teams
  = render 'team_tabs', user_group: @user_group
  = render 'shared/filters', presenter: @presenter,
    filter_tag_ids: @presenter.team_stats_presenter.filter_tag_ids,
    filter_start_time: @presenter.team_stats_presenter.filter_start_time,
    filter_end_time: @presenter.team_stats_presenter.filter_end_time
  = render 'boxes', presenter: @presenter
  = render 'summary', presenter: @presenter.team_stats_presenter

:javascript
  var lineChartData = #{raw @presenter.to_json(graphs: [{ type: :line_chart, data: :team_data }])};
